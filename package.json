{
  "name": "getempstatus",
  "version": "1.0.0",
  "description": "Express.js backend service for employee status processing with Sequelize, MySQL, and Redis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:if-empty": "node scripts/seed-if-empty.js",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "wait-for-db": "node scripts/wait-for-db.js"
  },
  "keywords": [
    "express",
    "sequelize",
    "mysql",
    "redis",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "mysql2": "^3.6.5",
    "redis": "^4.6.12",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
